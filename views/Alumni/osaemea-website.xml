<?xml version="1.0" encoding="utf-8" ?>
<odoo>
     <record id="osaemea-website" model="website.page">
        <field name="name">OSAEMEA Website</field>
        <field name="website_published">True</field>
        <field name="url">http://emeaalumniuae.com</field>
        <field name="type">qweb</field>
        <field name="key">website.journal</field>
        <field name="arch" type="xml">
            <t t-name="website.osaemea-website_template">
                 <t t-call="website.layout">
                     <div id="wrap" class="oe_structure">
                         <a href="http://emeaalumniuae.com" target="_blank">OSAEMEA Website</a>
                     </div>
                 </t>
            </t>
         </field>

     </record>

</odoo>



